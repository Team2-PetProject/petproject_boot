<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="FavoriteMapper">
<select id="itemList" parameterType="String" resultType="item_FavoriteDTO" >
select
*
from
item_favorite
where member_code=#{member_code}
</select>

<select id="tourList" parameterType="String" resultType="tour_FavoriteDTO" >
select
*
from
tour_favorite
where member_code=#{member_code}
</select>
</mapper>